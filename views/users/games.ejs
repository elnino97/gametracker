<% layout('layouts/profile')%>
<div class="row">
    <% for (let game of games) { %> 
    <div class="card" style="width: 18rem;">
        <img src="<%= game.background_image %> " class="card-img-top" alt="...">
        <div class="card-body">
          <h5 class="card-title"><%= game.name %> </h5>
          <p class="card-text"><%= game.released %> </p>
        </div>
      </div>
      <% } %> 
</div>